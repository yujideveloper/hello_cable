<% if current_account %>
  <p>Logged in as <%= current_account.email %></p>
  <ul>
    <li><%= link_to "Log out", rodauth.logout_path, method: :delete %></li>
  </ul>
<% else %>
  <ul>
    <li><%= link_to "Log in", rodauth.login_path %></li>
    <li><%= link_to "Create Account", rodauth.create_account_path %></li>
  </ul>
<% end %>
